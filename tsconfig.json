{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    ".astro/client.d.ts",
    "src/env.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verifyDefaultProps": true,
    "@components/*": ["./src/components/*"],
    "@data/*": ["./src/data/*"],
    "@layouts/*": ["./src/layouts/*"],
    "@lib/*": ["./src/lib/*"],
    "@styles/*": ["./src/styles/*"],
    "types": [
      "vite-plugin-pwa/vanillajs",
      "vite-plugin-pwa/info",
      "src/env.d.ts",
    ],
  }
}
{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "verifyDefaultProps": true,
    "@components/*": ["./src/components/*"],
    "@layouts/*": ["./src/layouts/*"],
    "types": [
      "vite-plugin-pwa/vanillajs",
      "vite-plugin-pwa/info",
    ],
  }
}